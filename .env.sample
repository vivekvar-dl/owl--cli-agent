# This is an example .env file.
# For local development, copy this file to .env and fill in your details.
# DO NOT COMMIT your .env file to version control.

# Required: Your Google Gemini API Key
# Get this from https://aistudio.google.com/app/apikey
GEMINI_API_KEY="YOUR_API_KEY_HERE"

# Optional: For the web_search tool, you need a Google Custom Search Engine ID
# See https://developers.google.com/custom-search/v1/overview
PROGRAMMABLE_SEARCH_ENGINE_ID="YOUR_SEARCH_ENGINE_ID_HERE"

# Optional: The GOOGLE_API_KEY is also required for the web_search tool.
# This can often be the same as your Gemini key, but it needs to have the "Custom Search API" enabled in your Google Cloud project.
GOOGLE_API_KEY="YOUR_GOOGLE_API_KEY_HERE" 